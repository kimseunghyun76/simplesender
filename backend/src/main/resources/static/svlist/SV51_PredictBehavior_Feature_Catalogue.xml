			<S100FC:valueType>text</S100FC:valueType>
		</S100FC:S100_FC_SimpleAttribute>	
		<S100FC:S100_FC_SimpleAttribute>
			<S100FC:name>Predict Behavior Time</S100FC:name>
			<S100FC:definition>Predict Behavior Time</S100FC:definition>
			<S100FC:code>psTime</S100FC:code>
			<S100FC:valueType>dateTime</S100FC:valueType>
		</S100FC:S100_FC_SimpleAttribute>
		<S100FC:S100_FC_SimpleAttribute>
			<S100FC:name>Latitude</S100FC:name>
