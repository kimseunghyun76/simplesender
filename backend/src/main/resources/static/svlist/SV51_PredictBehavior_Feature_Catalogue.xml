			</S100FC:attributeBinding>
			<S100FC:attributeBinding sequential="false">
				<S100FC:multiplicity>
					<S100Base:lower>1</S100Base:lower>
					<S100Base:upper xsi:nil="false" infinite="false">1</S100Base:upper>
				</S100FC:multiplicity>
				<S100FC:attribute ref="psLat"/>
			</S100FC:attributeBinding>
			<S100FC:attributeBinding sequential="false">
				<S100FC:multiplicity>
					<S100Base:lower>1</S100Base:lower>
					<S100Base:upper xsi:nil="false" infinite="false">1</S100Base:upper>
				</S100FC:multiplicity>
				<S100FC:attribute ref="psLong"/>
			</S100FC:attributeBinding>
			<S100FC:attributeBinding sequential="false">
				<S100FC:multiplicity>
					<S100Base:lower>1</S100Base:lower>
					<S100Base:upper xsi:nil="false" infinite="false">1</S100Base:upper>
				</S100FC:multiplicity>
